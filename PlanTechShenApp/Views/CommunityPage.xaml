<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PlanTechShenApp.Views.CommunityPage"
             Title="Community">
    
   
        <ContentPage.ToolbarItems>
            <ToolbarItem Clicked="OnToolbarItemClicked" />

            <ToolbarItem Text="Client Support"
                     Order="Secondary"
                         Clicked="ClientSupportClicked" />

            <ToolbarItem Text="Settings"
                     Order="Secondary"
                         Clicked="SettingsClicked"/>
        </ContentPage.ToolbarItems>
    <ImageButton Source="chat.png"
                    HorizontalOptions="Center"
                    VerticalOptions="CenterAndExpand"
                    Clicked="ImageClicked" />
  
</ContentPage>